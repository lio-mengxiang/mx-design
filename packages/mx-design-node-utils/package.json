{
  "name": "@mx-design/node-utils",
  "version": "0.1.3",
  "description": "library of functions for automating",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "license": "MIT",
  "files": [
    "types",
    "lib",
    "esm",
    "README.md",
    "README.zh.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "rimraf ./node_modules",
    "build:cjs": "rimraf ./lib && tsc && tsc -p ./tsconfigCommonJS.json",
    "build:esm": "rimraf ./esm && tsc && tsc -p ./tsconfig.json",
    "build:types": "rimraf ./types && tsc --outDir types -d --emitDeclarationOnly",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types"
  },
  "dependencies": {
    "@mx-design/web-utils": "0.1.8",
    "chalk": "4.0.0",
    "compressing": "1.6.2",
    "download": "8.0.0",
    "enquirer": "2.3.6",
    "fs-extra": "11.1.0",
    "ora": "5.4.0",
    "inquirer": "6.0.0",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/node": "18.11.12",
    "jsdom": "^20.0.3"
  }
}